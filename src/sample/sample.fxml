<SplitPane dividerPositions="0.5" minHeight="466.0" minWidth="659.0" orientation="VERTICAL" prefHeight="466.0" prefWidth="659.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <items>
      <VBox>
         <children>
            <MenuBar VBox.vgrow="NEVER">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <HBox alignment="CENTER_LEFT" prefHeight="37.0" spacing="5.0" style="-fx-padding: 6;" VBox.vgrow="NEVER">
               <children>
                  <JFXButton fx:id="btnOpen" maxWidth="103.0" onAction="#handleButtonOpen" prefHeight="25.0" prefWidth="103.0" text="Open" textAlignment="CENTER">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </JFXButton>
                  <JFXTextField fx:id="txtFilepath" editable="false" minWidth="250.0" promptText="File path" HBox.hgrow="ALWAYS">
                     <cursor>
                        <Cursor fx:constant="DEFAULT" />
                     </cursor></JFXTextField>
                  <JFXComboBox minWidth="196.0" prefHeight="25.0" prefWidth="156.0" promptText="J48" />
                  <JFXButton fx:id="btnClassify" buttonType="RAISED" disable="true" maxWidth="135.0" onAction="#handleButtonClassify" prefWidth="135.0" style="-fx-background-color: #800080;" text="Add &amp; Apply" textAlignment="CENTER" textFill="WHITE">
                     <HBox.margin>
                        <Insets left="3.0" />
                     </HBox.margin>
                  </JFXButton>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <TableView fx:id="tableView" prefHeight="179.0" prefWidth="657.0" />
            <GridPane VBox.vgrow="NEVER">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="100.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <JFXComboBox fx:id="cmbOutlook" maxWidth="1.7976931348623157E308" minWidth="100.0" GridPane.halignment="CENTER" />
                  <JFXComboBox fx:id="cmbPlay" maxWidth="1.7976931348623157E308" minWidth="100.0" GridPane.columnIndex="4" GridPane.halignment="CENTER" />
                  <JFXComboBox fx:id="cmbWindy" maxWidth="1.7976931348623157E308" minWidth="100.0" GridPane.columnIndex="3" GridPane.halignment="CENTER" />
                  <JFXComboBox fx:id="cmbHumidity" maxWidth="1.7976931348623157E308" minWidth="100.0" GridPane.columnIndex="2" GridPane.halignment="CENTER" />
                  <JFXButton fx:id="btnAgregar" disable="true" minWidth="60.0" onAction="#handleButtonAgregar" prefHeight="25.0" style="-fx-background-color: #35a1f4;" text="Agregar" textFill="WHITE" GridPane.columnIndex="5" GridPane.halignment="CENTER" GridPane.hgrow="NEVER" />
                  <JFXComboBox fx:id="cmbTemperature" maxWidth="1.7976931348623157E308" minWidth="100.0" GridPane.columnIndex="1" GridPane.halignment="CENTER" />
               </children>
            </GridPane>
         </children>
      </VBox>
      <JFXTextField fx:id="txtResult" alignment="CENTER" editable="false" maxHeight="171.0" prefHeight="171.0" prefWidth="657.0" promptText="Nada agregado">
         <font>
            <Font size="40.0" />
         </font></JFXTextField>
   </items>
</SplitPane>
